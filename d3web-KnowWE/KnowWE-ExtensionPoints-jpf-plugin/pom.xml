	<modelVersion>4.0.0</modelVersion>
	<groupId>de.uniwue.d3web</groupId>
	<artifactId>KnowWE-ExtensionPoints-jpf-plugin</artifactId>
	<name>KnowWE-ExtensionPoint</name>
